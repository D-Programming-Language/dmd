// empty.
                                 |

// basic types.
bool                             | bool
signed char                      | byte
unsigned char                    | ubyte
char                             | char
wchar_t                          | dchar
short                            | short
unsigned short                   | ushort
int                              | int
unsigned int                     | uint
long                             | core.stdc.config.c_long
unsigned long                    | core.stdc.config.c_ulong
float                            | float
double                           | double
long double                      | real
size_t                           | size_t
ptrdiff_t                        | ptrdiff_t
// Structs, S is a predefined empty struct.
S                                | S

// Flavors of type indirection.
int*                             | int*
const int*                       | const(int)*
const int* const                 | const(int*)
S*                               | S*
const S*                         | const(S)*
const S* const                   | const(S*)
int&                             | ref int
const int&                       | const ref int
S&                               | ref S
const S&                         | const ref S

// Function pointers.
void*(*)()                       | void* function()
int(*)(int)                      | int function(int)

// Type substitutions.
void*(*)(), void*                | void* function(), void*
void(*)(void*), void*            | void function(void*), void*
void*(*)(void**), void*          | void* function(void**), void*
void*(*)(void**), void**         | void* function(void**), void**
void*(*)(void*), void*(*)(void*) | void* function(void*), void* function(void*)

